<Window x:Class="AutoClicker15.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoClicker15"
        mc:Ignorable="d"
        Title="AutoClicker15" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button Name="btn_Sweets" Click="btn_Sweets_Click">
            Click Sweets
        </Button>
        <Button Grid.Row="1" Name="btn_AutoPlay" Click="btn_AutoPlay_Click">
            AutoPlay
        </Button>
    </Grid>
</Window>
